<h1>All Users</h1>

@if (@Model.Count == 0)
{
  <h3>No Users....</h3>
}
<ul>
  @foreach (var user in Model)
  {
    <li><a href="/users/@user.GetId()">@user.GetId()</a></li>
    <li>@user.GetName()</li>
    <li>@user.Getpassword()</li>
    <li>@user.GetId()</li>
    <li><a href="/users/edit/@user.GetId()">Edit</a></li>
    <li><a href="/users/delete/@user.GetId()">Delete</a></li>
  }
</ul>
<a href="/users/new">Add new user</a>
<br>
<a href="/users/delte">Delete user</a>
