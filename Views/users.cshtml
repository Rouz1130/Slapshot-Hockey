<h1>Hair Salon Database</h1>
<br>
<h1>User:</h1>
<ul>
  @if (Model.Count != 0)
  {
    @foreach (var user in Model)
    {
      <li><a href="/users/@user.GetId()">@user.GetName()</a>
       | <a href="/user/edit/@user.GetId()">Edit</a>
       | <a href="user/delete/user.GetId()">Delete</a> </li>
    }
  }
</ul>
<h4><a href="/users/new">Add User</a></h4>
<h4><a href="/">Home</a></h4>

<form action="/users/clear" method="post">
  <button type="submit">Delete</button>
</form>
