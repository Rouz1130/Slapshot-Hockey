<h1>All Teams</h1>

@if (@Model.Count == 0)
{
  <h3>No Teams....</h3>
}
<ul>
  @foreach (var team in Model)
  {
    <li>@team.GetName()</li>
    <li><a href="/teams/edit/@team.GetId()">Edit</a></li>
    <li><a href="/team/delete/@team.GetId()">Delete</a></li>
  }
</ul>
<a href="/teams/new">Add new team</a>
<br>
<a href="/teams/delete">Delete all team</a>
